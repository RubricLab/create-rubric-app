  },
  "dependencies": {
    "@inquirer/prompts": "^6.0.1",
    "@neondatabase/api-client": "^1.10.3",
    "@vercel/client": "^13.4.15",
    "boxen": "^8.0.1",
    "clear": "^0.1.0",
    "commander": "^12.1.0",
    "ejs": "^3.1.10",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@rubriclab/config": "*",
    "@rubriclab/package": "*",
    "next": "^14.2.14",
    "typescript": "^5.6.3"
  },
  "homepage": "https://github.com/RubricLab/create-rubric-app#readme",
  "keywords": [
    "agent",
